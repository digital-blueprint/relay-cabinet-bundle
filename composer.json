{
    "name": "dbp/relay-cabinet-bundle",
    "type": "symfony-bundle",
    "license": "AGPL-3.0-or-later",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "api-platform/core": "^3.4",
        "dbp/relay-base-person-bundle": "^0.2.26",
        "dbp/relay-blob-bundle": "^0.1.58",
        "dbp/relay-blob-library": "^0.2.12",
        "dbp/relay-core-bundle": "^0.1.190",
        "doctrine/dbal": "^3.8 || ^4.0",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/migrations": "^3.5",
        "doctrine/orm": "^2.18 || ^3.2",
        "monolog/monolog": "^3.8",
        "psr/http-client": "^1.0",
        "psr/log": "^1.1.4 || ^2.0 || ^3.0",
        "symfony/config": "^6.4",
        "symfony/console": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/event-dispatcher": "^6.4",
        "symfony/event-dispatcher-contracts": "^3.5",
        "symfony/filesystem": "^6.4",
        "symfony/framework-bundle": "^6.4",
        "symfony/http-client": "^6.4",
        "symfony/http-client-contracts": "^3.5",
        "symfony/http-foundation": "^6.4",
        "symfony/http-kernel": "^6.4",
        "symfony/messenger": "^6.4",
        "symfony/mime": "^6.4",
        "symfony/routing": "^6.4",
        "symfony/uid": "^6.4",
        "typesense/typesense-php": "^4.9.2 || ^5.0.2"
    },
    "conflict": {
        "php-http/httplug": "<2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.50",
        "phpstan/phpstan": "^2.0.0",
        "phpstan/phpstan-phpunit": "^2.0.0",
        "phpstan/phpstan-symfony": "^2.0.0",
        "phpunit/phpunit": "^10.1",
        "symfony/browser-kit": "^6.4",
        "symfony/phpunit-bridge": "^7.0.4",
        "symfony/error-handler":"^6.4",
        "symfony/monolog-bridge":"^6.4"
    },
    "autoload": {
        "psr-4": {
            "Dbp\\Relay\\CabinetBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dbp\\Relay\\CabinetBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": false,
            "php-http/discovery": false
        }
    },
    "scripts": {
        "test": [
            "@php vendor/bin/phpunit"
        ],
        "phpstan": [
            "@php vendor/bin/phpstan analyze --ansi"
        ],
        "lint": [
            "@composer run cs",
            "@composer run phpstan"
        ],
        "cs-fix": [
            "@php vendor/bin/php-cs-fixer --ansi fix"
        ],
        "cs": [
            "@php vendor/bin/php-cs-fixer --ansi fix --dry-run --diff"
        ],
        "coverage": [
            "@php -dxdebug.mode=coverage vendor/bin/phpunit --coverage-html _coverage"
        ]
    }
}
